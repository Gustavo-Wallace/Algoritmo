create database tabelas;

create table conta(
id_conta int(3) not null auto_increment,
numero int(10) not null,
agencia varchar(10) not null,
primary key(id_conta)
);

create table cliente(
id_cliente int(3) not null auto_increment,
nome varchar(30) not null,
id_conta int(3),
primary key(id_cliente),
constraint fk_conta foreign key(id_conta) references conta(id_conta)
);

insert into conta values (1,1000010,'ponts');
select * from conta;
drop table conta;
show tables;
